#lang racket
(define (qbrt x)
  (define (qube x) (* x x x))
  (define (square x) (* x x))
  (define (improve guess)
      (/ (+ (/ x (square guess))
            (* 2 guess))
         3))
  (define (good-enough? guess)
      (< (abs (- (qube guess) x)) 0.001))
  (define (qbrt-iter guess)
        (if (good-enough? guess)
        guess
        (qbrt-iter (improve guess))))
  (qbrt-iter 1.0)
)

(qbrt 27)

